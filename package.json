{
  "name": "drone-tx",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.11.4",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-nipple": "^1.0.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.3",
    "react-websocket": "^2.1.0",
    "styled-components": "^5.3.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "format": "npm run format:prettier",
    "format:prettier": "prettier --config ./.prettierrc.json --write \"src/**/*.{js,jsx}\"",
    "lint": "npm run lint:js",
    "lint:js": "eslint --fix \"src/**/*.{js,jsx}\"",
    "lint:staged": "./node_modules/.bin/eslint --fix",
    "precommit": "pretty-quick --staged && lint-staged",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "npm run lint:staged"
    ]
  },
  "browserslist": [
    "defaults",
    "not ie <= 11"
  ],
  "devDependencies": {
    "@welldone-software/why-did-you-render": "^6.1.4",
    "eslint": "^7.27.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "prettier": "2.3.0",
    "pretty-quick": "^3.1.0"
  }
}
